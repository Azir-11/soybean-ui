{
  "name": "soybean-unify",
  "type": "module",
  "version": "0.0.1",
  "description": "SoybeanUnify is a ui library for Vue3, which is based on shadcn-vue and UnoCSS.",
  "author": {
    "name": "Soybean",
    "email": "soybeanjs@outlook.com",
    "url": "https://github.com/soybeanjs"
  },
  "license": "MIT",
  "homepage": "https://github.com/soybeanjs/soybean-unify",
  "repository": {
    "url": "https://github.com/soybeanjs/soybean-unify.git"
  },
  "bugs": {
    "url": "https://github.com/soybeanjs/soybean-unify/issues"
  },
  "keywords": ["UI", "UnoCSS"],
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build",
    "cleanup": "soy cleanup",
    "commit": "soy git-commit",
    "dev": "vite",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "soy release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "soy update-pkg"
  },
  "dependencies": {
    "@soybean-unify/shadcn-ui": "workspace:*",
    "@soybean-unify/ui": "workspace:*",
    "@unocss/reset": "0.62.1",
    "@vueuse/core": "10.11.1",
    "lucide-vue-next": "0.427.0",
    "vue": "3.4.37"
  },
  "devDependencies": {
    "@soybean-unify/unocss-preset": "workspace:*",
    "@soybean-unify/unocss-preset-shadcn": "workspace:*",
    "@soybeanjs/cli": "1.1.0",
    "@soybeanjs/eslint-config": "1.4.0",
    "@types/node": "22.2.0",
    "@unocss/eslint-config": "^0.62.1",
    "@vitejs/plugin-vue": "5.1.2",
    "@vitejs/plugin-vue-jsx": "4.0.0",
    "eslint": "9.9.0",
    "eslint-plugin-vue": "9.27.0",
    "lint-staged": "15.2.8",
    "simple-git-hooks": "2.11.1",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "unocss": "0.62.1",
    "unocss-preset-animations": "1.1.0",
    "vite": "5.4.0",
    "vite-plugin-vue-devtools": "7.3.8",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "2.0.29"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
